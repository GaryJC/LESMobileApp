{
  "name": "lesmobileapp",
  "version": "1.1.1",
  "main": "index.js",
  "scripts": {
    "start": "expo start --dev-client",
    "android": " expo run:android",
    "ios": "APP_ENV=preview expo run:ios",
    "web": "expo start --web",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "eas:preview:ios": "EXPO_NO_CAPABILITY_SYNC=1 APP_ENV=preview eas build --profile preview --platform ios --clear-cache",
    "eas:preview:android": "EXPO_NO_CAPABILITY_SYNC=1 APP_ENV=preview eas build --profile preview --platform android --clear-cache",
    "eas:preview:all": "EXPO_NO_CAPABILITY_SYNC=1 APP_ENV=preview eas build --profile preview --platform all --clear-cache",
    "eas:prod:ios": "EXPO_NO_CAPABILITY_SYNC=1 eas build --profile production --platform ios --clear-cache",
    "eas:prod:android": "EXPO_NO_CAPABILITY_SYNC=1 eas build --profile production --platform android --clear-cache",
    "eas:prod:all": "EXPO_NO_CAPABILITY_SYNC=1 eas build --profile production --platform all --clear-cache",
    "eas:update:preview": "EXPO_NO_CAPABILITY_SYNC=1 APP_ENV=preview eas update --branch preview",
    "eas:update:prod": "eas update --branch production"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@gorhom/bottom-sheet": "^4.4.6",
    "@invertase/react-native-apple-authentication": "^2.3.0",
    "@notifee/react-native": "^7.8.0",
    "@react-native-clipboard/clipboard": "^1.12.1",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-community/netinfo": "11.1.0",
    "@react-native-firebase/analytics": "^18.3.0",
    "@react-native-firebase/app": "^18.3.0",
    "@react-native-firebase/auth": "^18.3.0",
    "@react-native-firebase/messaging": "^18.3.0",
    "@react-native-google-signin/google-signin": "^10.0.1",
    "@react-native-twitter-signin/twitter-signin": "^1.2.0",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/drawer": "^6.6.3",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@walletconnect/react-native-compat": "^2.14.0",
    "@web3modal/ethers-react-native": "^2.0.0",
    "axios": "^1.4.0",
    "ethers": "^6.13.1",
    "expo": "^50.0.0",
    "expo-application": "~5.8.4",
    "expo-constants": "~15.4.6",
    "expo-secure-store": "~12.8.1",
    "expo-splash-screen": "~0.26.5",
    "expo-sqlite": "~13.4.0",
    "expo-status-bar": "~1.11.1",
    "expo-updates": "~0.24.13",
    "js-base64": "^3.7.5",
    "les-im-components": "^1.1.56",
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "nativewind": "^2.0.11",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-bouncy-checkbox-group": "^0.1.1",
    "react-native-gesture-handler": "~2.9.0",
    "react-native-get-random-values": "~1.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "^3.10.1",
    "react-native-popover-view": "^5.1.8",
    "react-native-reanimated": "~3.6.2",
    "react-native-safe-area-context": "4.8.2",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "14.1.0",
    "react-native-toast-message": "^2.1.7",
    "react-native-webview": "13.6.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "react-native-svg-transformer": "^1.5.0",
    "tailwindcss": "3.3.2"
  },
  "private": true
}
